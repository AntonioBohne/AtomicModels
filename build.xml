<?xml version="1.0" encoding="UTF-8"?>
<project name="Chemistry" default="default" basedir=".">
    <description>Builds, tests, and runs the project Chemistry.</description>
    <import file="nbproject/build-impl.xml"/>
    
    <target name="-post-jar">
        <jar jarfile="AtomicModel.jar">
            <zipfileset src="${dist.jar}" excludes="META-INF/*" /> 
            <zipfileset src="dist/lib/sqlite-jdbc-3.21.0.jar" excludes="META-INF/*" /> 
            <manifest>
                <attribute name="Main-Class" value="chemistry.start.JFXUI"/>
            </manifest>
        </jar>
    </target>
</project>
